{
  "name": "redux-analytics",
  "version": "0.3.1",
  "description": "Analytics middleware for Redux",
  "main": "lib/index.js",
  "scripts": {
    "test": "babel-istanbul cover _mocha && babel-istanbul check-coverage --branches 100",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "build": "rm -rf lib/ && ./node_modules/.bin/babel -d lib/ src/",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markdalgleish/redux-analytics.git"
  },
  "author": "Mark Dalgleish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markdalgleish/redux-analytics/issues"
  },
  "homepage": "https://github.com/markdalgleish/redux-analytics#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-istanbul": "^0.3.20",
    "chai": "^3.3.0",
    "coveralls": "^2.11.6",
    "mocha": "^2.3.3",
    "redux": "^3.0.2",
    "sinon": "^1.17.1"
  },
  "dependencies": {
    "flux-standard-action": "^0.6.0"
  }
}
